{
  "license": "MIT",
  "name": "egorsmkv/yii2-blank",
  "description": "A simple Yii 2 project with Twig template engine.",
  "type": "project",
  "authors": [
    {
      "name": "Yehor Smoliakov",
      "email": "egorsmkv@gmail.com",
      "homepage": "https://yehor.net",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "yiisoft/yii2": "2.0.12",
    "yiisoft/yii2-bootstrap": "2.0.6",
    "yiisoft/yii2-twig": "2.1.0",
    "yidas/yii2-composer-bower-skip": "2.0.12"
  },
  "require-dev": {
    "yiisoft/yii2-debug": "2.0.9"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "installer-paths": {
        "bower-asset-library": "vendor/bower"
      }
    }
  },
  "scripts": {
    "post-create-project-cmd": [
      "yii\\composer\\Installer::postCreateProject"
    ]
  },
  "extra": {
    "yii\\composer\\Installer::postCreateProject": {
      "setPermission": [
        {
          "runtime": "0777",
          "web/assets": "0777",
          "yii": "0755"
        }
      ],
      "generateCookieValidationKey": [
        "config/web.php"
      ]
    }
  }
}
